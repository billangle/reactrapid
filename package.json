{
  "name": "rapidcc",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "env-cmd -f env/.env vite",
    "start:prod": "env-cmd -f env/.env.prod vite",
    "start:dev": "env-cmd -f env/.env.dev vite",
    "start:stg": "env-cmd -f env/.env.stg vite",
    "build:prod": "tsc && env-cmd -f env/.env.prod vite build",
    "build:dev": "tsc && env-cmd -f env/.env.dev vite build",
    "build:stg": "tsc && env-cmd -f env/.env.stg vite build",
    "deploy:prod": "aws --profile $MYPROFILE s3 sync dist/ s3://app.rapidright.net",
    "deploy:dev": "aws --profile $MYPROFILE s3 sync dist/ s3://dev.rapidright.net",
    "deploy:stg": "aws --profile $MYPROFILE s3 sync dist/ s3://stg.rapidright.net",
    "preview": "vite preview",
    "install-and-start": "npm i && npm run start",
    "win-deploy": "git checkout dev && git pull && npm install && aws configure sso && npm run build && aws --profile %myprofile% s3 sync dist/ s3://dev.rapidedgecc.net",
    "mac-deploy": "git checkout dev && git pull && npm install && aws configure sso && npm run build && aws --profile $MYPROFILE s3 sync dist/ s3://dev.rapidedgecc.net",
    "eslint-debug": "npx eslint --debug --fix src/**/*.{ts,tsx}",
    "eslint": "npx eslint --fix --quiet src/**/*.{ts,tsx}",
    "eslint-tofile": "npx eslint -o lintoutput.txt --fix src/**/*.{ts,tsx}",
    "test": "vitest run --silent",
    "test:ui": "vitest --ui --silent",
    "coverage": "vitest run --coverage --silent",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-amplify/core": "^5.2.0",
